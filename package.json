{
  "name": "gavdaly",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "check": "astro check",
    "typecheck": "astro check",
    "lint": "eslint 'src/**/*.{astro,ts,tsx}'",
    "format": "prettier --write .",
    "lint:md": "markdownlint-cli2",
    "lint:spell": "cspell --no-progress --no-summary '**/*.{md,mdx,astro,ts,tsx}'",
    "typecheck:workers": "tsc --noEmit -p tsconfig.workers.json",
    "lint:all": "npm run lint && npm run lint:md && npm run lint:spell",
    "astro": "astro",
    "cf:dev": "wrangler pages dev --compatibility-date=2024-12-01",
    "d1:migrate": "wrangler d1 migrations apply your-d1-db",
    "d1:console": "wrangler d1 execute your-d1-db --local --command '.tables'"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/solid-js": "^5.0.1",
    "@astrojs/tailwind": "^5.1.3",
    "astro": "^5.0.5",
    "astro-robots-txt": "^1.0.0",
    "astro-seo": "0.8.4",
    "marked": "^14.0.0",
    "solid-js": "^1.9.3",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "cspell": "^8.16.0",
    "depcheck": "^1.4.7",
    "eslint": "9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "1.2.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "markdownlint-cli2": "^0.15.0",
    "prettier": "3.3.3",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.6.6",
    "remark-cli": "^12.0.1",
    "remark-validate-links": "^13.0.2",
    "ts-prune": "^0.10.3"
  }
}
